#!/bin/bash

set -e

make down
docker volume rm icms_pgdata
make migrate
make manage args="create_icms_groups"
make add_dummy_data
make manage args="set_icms_sites http://caseworker http://export-a-certificate http://import-a-licence"

echo "Restarting containers in the background"
make debug

echo "collecting static content"
make collectstatic
